<?xml version="1.0" encoding="utf-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
  <ribbon startFromScratch="false">
    <tabs>
      <tab id="MyTab1" label="MyAddin" keytip="M">


        <group id="Group1" label="ObjectSetting">
          <button id="Button1-1" label="Widths" keytip="W" imageMso="SizeToControlWidth" size="large" onAction="AlignBlockWidths" />
          <button id="Button1-2" label="Heights" keytip="H" imageMso="SizeToControlHeight" size="large" onAction="AlignBlockHeights" />
          <button id="Button1-3" label="Sizes" keytip="S" imageMso="SizeToControlHeightAndWidth" size="large" onAction="AlignBlockSizes" />
        </group>

        <group id="Group2" label="MoveObjectsToAlign">
          <button id="Button2-1" label="ToLeft" keytip="2L" imageMso="ObjectsAlignLeft" size="large" onAction="MoveLeftToLastRight" />
          <button id="Button2-2" label="ToRight" keytip="2R" imageMso="ObjectsAlignRight" size="normal" onAction="MoveRightToLastLeft" />
          <button id="Button2-3" label="ToUp" keytip="2U" imageMso="ObjectsAlignTop" size="normal" onAction="MoveUpToLastBottom" />
          <button id="Button2-4" label="ToDown" keytip="2D" imageMso="ObjectsAlignBottom" size="normal" onAction="MoveDownToLastTop" />
        </group>

        <group id="Group3" label="ChangeObjects">
          <button id="Button3-1" label="SwapObj" keytip="OS" imageMso="CircularReferences" size="large" onAction="SwapObj" />
          <button id="Button3-2" label="ReplaceObj" keytip="OR" imageMso="OpenSharedCalendar" size="large" onAction="ReplaceObj" />
        </group>

        <group id="Group4" label="CreateIncircle">
          <button id="Button4-1" label="ForTriangle" keytip="TR" imageMso="ShapeIsoscelesTriangle" size="normal" onAction="CreateIncircleForTriangle" />
          <button id="Button4-2" label="ForInvertedTriangle" keytip="IT" imageMso="MacroPlay" size="normal" onAction="CreateIncircleForInvertedTriangle" />
        </group>

      </tab>
    </tabs>
  </ribbon>
</customUI>