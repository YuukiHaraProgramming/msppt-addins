<?xml version="1.0" encoding="utf-8"?>
<customUI xmlns="http://schemas.microsoft.com/office/2006/01/customui">
<ribbon startFromScratch="false">
<tabs>
<tab id="MyTab1" label="CustomUI">


<group id="Group1" label="AlignBlocks">
<button id="Button1-1" label="Widths" imageMso="MacroPlay" size="normal" onAction="AlignBlockWidths" />
<button id="Button1-2" label="Heights" imageMso="MacroPlay" size="normal" onAction="AlignBlockHeights" />
<button id="Button1-3" label="Sizes" imageMso="MacroPlay" size="normal" onAction="AlignBlockSizes" />
</group>

<group id="Group2" label="MoveObjectsToAlign">
<button id="Button2-1" label="ToLeft" imageMso="ObjectsAlignLeft" size="large" onAction="MoveLeftToLastRight" />
<button id="Button2-2" label="ToRight" imageMso="ObjectsAlignRight" size="normal" onAction="MoveRightToLastLeft" />
<button id="Button2-3" label="ToUp" imageMso="ObjectsAlignTop" size="normal" onAction="MoveUpToLastBottom" />
<button id="Button2-4" label="ToDown" imageMso="ObjectsAlignBottom" size="normal" onAction="MoveDownToLastTop" />
</group>

</tab>
</tabs>
</ribbon>
</customUI>